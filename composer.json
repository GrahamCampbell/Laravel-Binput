{
    "name": "graham-campbell/binput",
    "description": "Binput Is An Input Protector For Laravel 4.1",
    "keywords": ["laravel", "framework", "better input", "better", "input", "Binput", "Laravel Binput", "Laravel-Binput", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "graham-campbell/security": "0.5.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.1.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\Binput": "src/",
            "GrahamCampbell\\Tests\\Binput": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
