{
    "name": "graham-campbell/binput",
    "description": "Binput Is An Input Protector For Laravel",
    "keywords": ["laravel", "framework", "better input", "better", "input", "security", "xss", "Binput", "Laravel Binput", "Laravel-Binput", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0|^7.0",
        "illuminate/http": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "graham-campbell/security": "^7.1"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4",
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Binput\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Binput\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrahamCampbell\\Binput\\BinputServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
