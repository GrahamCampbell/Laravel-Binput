{
    "name": "graham-campbell/binput",
    "description": "Binput Is An Input Protector For Laravel 5",
    "keywords": ["laravel", "framework", "better input", "better", "input", "security", "Binput", "Laravel Binput", "Laravel-Binput", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/http": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "graham-campbell/security": "^4.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^1.0",
        "graham-campbell/testbench": "^4.0",
        "mockery/mockery": "dev-master#c90a17247147543081e4d00f46911e422b49e583",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Binput\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Binput\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
